{
  "name": "acquia/acquia-ra-composer",
  "description": "Project json for Acquia Remote Administration Composer builds",
  "type": "project",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "_comment": "The countries geoJSON files as a library",
      "type": "package",
      "package": {
          "name": "mledoze/countries",
          "version": "dev-master",
          "type": "drupal-library",
          "dist": {
              "url": "https://github.com/mledoze/countries/archive/master.zip",
              "type": "zip"
          }
      }
    }
  ],
  "require": {
    "composer/installers": "^1.7",
    "cweagans/composer-patches": "^1.6.0",
    "drupal/core-composer-scaffold": "^8.8",
    "drupal/core-recommended": "^8.8",
    "drupal/admin_toolbar": "^2.0",
    "drush/drush": "^8.1",
    "drupal/bootstrap_barrio": "^4.14",
    "drupal/bootstrap_sass": "^1.5",
    "drupal/paragraphs": "^1.3",
    "drupal/search_api": "^1.9",
    "drupal/entity": "^1.0",
    "drupal/addtoany": "^1.10",
    "drupal/forms_steps": "^1.0@RC",
    "drupal/ds": "^3.1",
    "drupal/geofield_map": "^2.13",
    "drupal/back_to_top": "^1.0",
    "drupal/address": "~1.0",
    "drupal/contact_block": "^1.4",
    "drupal/shrinktheweb": "^1.1",
    "drupal/entity_usage": "^2.0@alpha",
    "drupal/pdfpreview": "^1.0@RC",
    "drupal/colorbox": "^1.4",
    "drupal/social_api": "^2.0",
    "drupal/social_auth_twitter": "^2.0",
    "drupal/social_auth_facebook": "~2.0",
    "drupal/social_auth_google": "^2.0",
    "drupal/social_auth": "^2.0",
    "drupal/acquia_connector": "^1.14",
    "drupal/facets": "^1.1",
    "drupal/video_embed_field": "^2.0",
    "drupal/search_api_solr": "^1.2",
    "drupal/field_group": "^3.1",
    "drupal/field_group_ajaxified_multipage": "^1.0",
    "drupal/conditional_fields": "^1.0@alpha",
    "drupal/chosen": "^2.6",
    "drupal/synonyms": "1.x-dev",
    "drupal/better_field_descriptions": "^1.4",
    "drupal/geocoder": "^2.0@beta",
    "drupal/leaflet_more_maps": "^1.0@alpha",
    "drupal/better_exposed_filters": "^5.0",
    "drupal/taxonomy_publish": "^1.1",
    "drupal/taxonomy_access_fix": "^2.6",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/form_mode_manager": "^1.3",
    "drupal/masquerade": "^2.0@beta",
    "drupal/hide_revision_field": "^2.1",
    "drupal/entity_browser": "^2.0",
    "drupal/content_browser": "^1.0@alpha",
    "drupal/label_help": "1.x-dev",
    "drupal/field_states_ui": "^1.0@RC",
    "drupal/config_split": "^1.4",
    "drupal/entity_reference_facet_link": "^1.0@beta",
    "drupal/menu_link_attributes": "^1.0",
    "drupal/readonlymode": "^1.0@RC",
    "drupal/facets_pretty_paths": "^1.0@beta",
    "drupal/redirect": "^1.2",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/google_analytics": "^2.3",
    "league/oauth2-client": "^2.3",
    "drupal/account_modal": "^1.0@beta",
    "drupal/super_login": "^1.2",
    "drupal/r4032login": "^1.1",
    "drupal/registration_role": "^1.0@beta",
    "drupal/recaptcha": "^2.3",
    "league/oauth2-google": "^2.2",
    "drupal/simplify": "^1.0",
    "drupal/rules": "^3.0@alpha",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/honeypot": "^1.29",
    "drupal/leaflet_countries": "^1.1",
    "mledoze/countries": "^2.1",
    "drupal/geolocation": "^3.0",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/views_entity_form_field": "^1.0@beta",
    "drupal/asset_injector": "^2.4",
    "drupal/leaflet_mapbox": "1.x-dev",
    "drupal/username_enumeration_prevention": "^1.0",
    "drupal/http_response_headers": "^2.0@beta",
    "drupal/condition_query": "^1.2",
    "drupal/memcache": "^2.0",
    "drupal/pathauto": "^1.8",
    "drupal/content_moderation_notifications": "^3.2",
    "drupal/leaflet": "^2.1"
  },
  "require-dev": {
    "drupal/devel": "^1.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/taxonomy_manager": "^1.0@alpha"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      },
      "allowed-packages": [
        "drupal/core"
      ],
      "file-mapping": {
        "[web-root]/.htaccess": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/htaccess",
          "overwrite": false
        }
      }
    },
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "patches": {
      "drupal/core": {
        "getEntityTranslation() throws exception on null": "https://www.drupal.org/files/issues/2018-09-05/2951493-15.patch",
        "Make exposed filter values available as tokens": "https://www.drupal.org/files/issues/2018-05-14/make_exposed_filter-2761273-13.patch"
      },
      "drupal/address": {
        "Integrate Address with Search API": "https://www.drupal.org/files/issues/2018-06-07/address-search-api-full-name-properties-2812659-37.patch"
      },
      "drupal/label_help": {
        "Improve Drupal 8 field compatibility": "https://www.drupal.org/files/issues/2018-06-05/label_help-2948501-5.patch"
      },
      "drupal/paragraphs": {
        "Error on upload via entity browser": "https://www.drupal.org/files/issues/paragraphs-missing-langcode-2901390-9.patch"
      },
      "drupal/masquerade": {
        "Unmasquerade link always appears": "https://www.drupal.org/files/issues/2018-07-11/unmasquerade_link-2695779-17.patch"
      },
      "drupal/rules": {
        "Condition UserHasRole throws an error": "https://www.drupal.org/files/issues/2816157-10.patch"
      },
      "drupal/leaflet_countries": {
        "Add compatibility with latest leaflet version and support for country outlines to be clickable and navigate to another url.": "https://www.drupal.org/files/issues/2020-01-08/leaflet_countries-grouped-entities-popup-3086578-11.patch"
      },
      "drupal/facets": {
        "Hierarchical Facets using Dropdown Widget": "https://www.drupal.org/files/issues/2019-07-08/3058431-5-facets-add-hierarchy-in-dropdown-widget.patch"
      },
      "drupal/condition_query": {
        "Enable wildcard": "https://www.drupal.org/files/issues/2018-07-18/2986466-2-condition_query_wildcard.patch"
      }
    }
  },
  "scripts": {
  },
  "enable-patching": true
}
