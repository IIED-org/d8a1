{
  "name": "acquia/acquia-ra-composer",
  "description": "Project json for Acquia Remote Administration Composer builds",
  "type": "project",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-recommended": "^9",
    "drupal/admin_toolbar": "^2.0",
    "drush/drush": "^10",
    "drupal/bootstrap_barrio": "^4.14",
    "drupal/bootstrap_sass": "^5.0",
    "drupal/paragraphs": "^1.3",
    "drupal/search_api": "^1.9",
    "drupal/entity": "^1.0",
    "drupal/addtoany": "^1.10",
    "drupal/forms_steps": "^1.0@RC",
    "drupal/ds": "^3.1",
    "drupal/geofield_map": "^2.13",
    "drupal/back_to_top": "^1.0",
    "drupal/address": "~1.0",
    "drupal/contact_block": "^1.4",
    "drupal/entity_usage": "^2.0@alpha",
    "drupal/pdfpreview": "^1.0@RC",
    "drupal/colorbox": "^1.4",
    "drupal/social_api": "^3.0",
    "drupal/social_auth_twitter": "^2.x-dev@dev",
    "drupal/social_auth_facebook": "^3.0",
    "drupal/social_auth_google": "^3.0",
    "drupal/social_auth": "^3.0",
    "drupal/acquia_connector": "^3.0",
    "drupal/facets": "^1.1",
    "drupal/video_embed_field": "^2.0",
    "drupal/search_api_solr": "^4.2",
    "drupal/field_group": "^3.1",
    "drupal/conditional_fields": "^4.0",
    "drupal/chosen": "^2.6",
    "drupal/synonyms": "1.x-dev",
    "drupal/better_field_descriptions": "^1.4",
    "drupal/geocoder": "^3.20",
    "drupal/leaflet_more_maps": "^1.0@alpha",
    "drupal/better_exposed_filters": "^5.0",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/form_mode_manager": "^2.0",
    "drupal/masquerade": "^2.0@beta",
    "drupal/hide_revision_field": "^2.1",
    "drupal/entity_browser": "^2.0",
    "drupal/content_browser": "^1.0@alpha",
    "drupal/field_states_ui": "^2.0",
    "drupal/config_split": "^1.4",
    "drupal/entity_reference_facet_link": "^1.0@beta",
    "drupal/menu_link_attributes": "^1.0",
    "drupal/readonlymode": "^1.1",
    "drupal/facets_pretty_paths": "^1.0@beta",
    "drupal/redirect": "^1.2",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/google_analytics": "^2.3",
    "league/oauth2-client": "^2.3",
    "drupal/account_modal": "^1.0@beta",
    "drupal/super_login": "^1.2",
    "drupal/r4032login": "^2.1",
    "drupal/registration_role": "^1.0@beta",
    "drupal/recaptcha": "^3.0",
    "league/oauth2-google": "^3.0",
    "drupal/simplify": "^1.0",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/honeypot": "^2.0",
    "mledoze/countries": "^4",
    "drupal/geolocation": "^3.0",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/views_entity_form_field": "^1.0@beta",
    "drupal/asset_injector": "^2.4",
    "drupal/leaflet_mapbox": "1.x-dev",
    "drupal/username_enumeration_prevention": "^1.0",
    "drupal/http_response_headers": "^2.0@beta",
    "drupal/condition_query": "1.x-dev@dev",
    "drupal/memcache": "^2.0",
    "drupal/pathauto": "^1.8",
    "drupal/content_moderation_notifications": "^3.2",
    "drupal/leaflet": "^2.1",
    "drupal/block_class": "^1.3",
    "drupal/upgrade_status": "^3.0",
    "drupal/acquia_search": "^3.0",
    "drupal/core-project-message": "^9"
  },
  "require-dev": {
    "drupal/stage_file_proxy": "^1.0"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      },
      "allowed-packages": [
        "drupal/core"
      ],
      "file-mapping": {
        "[web-root]/.htaccess": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/htaccess",
          "overwrite": false
        }
      }
    },
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "patches": {
      "drupal/core": {
        "getEntityTranslation() throws exception on null": "https://www.drupal.org/files/issues/2018-09-05/2951493-15.patch",
        "Make exposed filter values available as tokens": "https://www.drupal.org/files/issues/2018-05-14/make_exposed_filter-2761273-13.patch"
      },
      "drupal/address": {
        "Integrate Address with Search API": "https://www.drupal.org/files/issues/2018-06-07/address-search-api-full-name-properties-2812659-37.patch"
      },
      "drupal/paragraphs": {
        "Error on upload via entity browser": "https://www.drupal.org/files/issues/2021-05-03/paragraphs-2901390-82-withtests.patch"
      },
      "drupal/masquerade": {
        "Unmasquerade link always appears": "https://www.drupal.org/files/issues/2018-07-11/unmasquerade_link-2695779-17.patch"
      },
      "drupal/condition_query": {
        "Enable wildcard": "https://www.drupal.org/files/issues/2021-06-04/2986466-25.patch"
      },
      "drupal/readonlymode": {
        "Got fatal error when running this module": "https://www.drupal.org/files/issues/2020-12-23/readonlymode-string_not_array-3115119-9.patch"
      }
    }
  },
  "scripts": {
  },
  "enable-patching": true
}
